{
    "webroot": "wwwroot",
    "version": "1.0.0-*",

    "dependencies": {
        "Microsoft.AspNet.Mvc": "6.0.0-beta7",
        "Microsoft.AspNet.Mvc.WebApiCompatShim": "6.0.0-beta7",
        "Microsoft.AspNet.Server.IIS": "1.0.0-beta7",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta7",
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta7",
        "Microsoft.Framework.Configuration.Json": "1.0.0-beta7",
        "Microsoft.Framework.Configuration.UserSecrets": "1.0.0-beta7",
        "Neo4jClient": "1.1.0.10",
        "Newtonsoft.Json": "6.0.6",
        "Swashbuckle": "6.0.0-beta7",
        "EntityFramework": "6.1.3",
        "bootstrap": "3.3.5",
        "Microsoft.Net.Http": "2.2.29",
        "jQuery": "2.1.4",
        "WebGrease": "1.6.0",
        "Microsoft.AspNet.WebApi": "5.2.3",
        "Microsoft.AspNet.Identity.EntityFramework": "2.2.1",
        "Microsoft.Azure.ActiveDirectory.GraphClient": "2.1.0"
    },

    "commands": {
        "web": "Microsoft.AspNet.Hosting --config hosting.ini",
        "kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --config hosting.ini"
    },

    "frameworks": {
        "dnx451": {
            "frameworkAssemblies": {
                "Microsoft.CSharp": "4.0.0.0",
                "mscorlib": "4.0.0.0",
                "System": "4.0.0.0",
                "System.Core": "4.0.0.0"
            }
        }
    },

    "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
    ],
    "publishExclude": [
        "node_modules",
        "bower_components",
        "**.xproj",
        "**.user",
        "**.vspscc"
    ]
}
